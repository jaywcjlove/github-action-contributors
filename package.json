{
  "name": "github-action-contributors",
  "version": "1.3.4",
  "description": "Github action generates dynamic image URL for contributor list to display it!.",
  "private": true,
  "scripts": {
    "prepare": "husky install && npm run build",
    "build": "ncc build src/index.ts -o lib",
    "watch": "ncc watch src/index.ts -o lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/github-action-contributors"
  },
  "keywords": [],
  "license": "MIT",
  "lint-staged": {
    "*.ts?(x)": [
      "npm run build"
    ]
  },
  "dependencies": {
    "@actions/core": "~1.10.0",
    "@actions/github": "~5.1.0",
    "image-size": "~1.0.1",
    "image2uri": "~1.1.0"
  },
  "devDependencies": {
    "@kkt/ncc": "~1.0.12",
    "husky": "~8.0.0",
    "lint-staged": "~13.1.0"
  }
}
