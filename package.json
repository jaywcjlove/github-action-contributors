{
  "name": "github-action-contributors",
  "version": "1.0.4",
  "description": "Github action generates dynamic image URL for contributor list to display it!.",
  "main": "./lib/index.js",
  "scripts": {
    "prepare": "husky install && npm run package",
    "package": "ncc build src/index.ts -o lib",
    "start": "node lib/index.js",
    "watch": "tsbb watch --disable-babel",
    "build": "tsbb build --disable-babel",
    "test": "npm run build && tsbb test",
    "coverage": "npm run build && tsbb test --coverage --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/github-action-contributors"
  },
  "keywords": [],
  "license": "MIT",
  "lint-staged": {
    "*.ts?(x)": [
      "npm run package"
    ]
  },
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/github": "5.0.0",
    "image-size": "1.0.1",
    "image2uri": "1.1.0"
  },
  "devDependencies": {
    "@vercel/ncc": "0.33.3",
    "husky": "7.0.4",
    "lint-staged": "12.3.1",
    "tsbb": "^3.5.4"
  }
}
